<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.m.myapplication">

    <application
        android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BookEdit" android:label="@string/app_name" />

        <provider android:name=".MyContentProvider" android:exported="true"
            android:readPermission="true" android:writePermission="true"
            android:authorities="com.example.m.myapplication.MyContentProvider" />
    </application>

</manifest>